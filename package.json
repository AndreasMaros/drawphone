{
    "name": "drawphone",
    "version": "1.0.0",
    "description": "Telephone with pictures",
    "private": true,
    "scripts": {
        "start": "run-s build:prod start:prod",
        "dev": "run-p start:dev build:dev",
        "start:dev": "nodemon --exec npm run start:prod --ignore public/",
        "build:dev": "webpack --mode development --watch",
        "start:prod": "node --experimental-json-modules ./server/bin/www",
        "build:prod": "webpack --mode production",
        "test": "jest",
        "lint:staged": "pretty-quick --staged"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tannerkrewson/drawphone.git"
    },
    "author": "Tanner Krewson",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tannerkrewson/drawphone/issues"
    },
    "homepage": "https://github.com/tannerkrewson/drawphone#readme",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.1",
        "assets-webpack-plugin": "^6.0.4",
        "aws-sdk": "^2.798.0",
        "blueimp-canvas-to-blob": "^3.28.0",
        "body-parser": "^1.19.0",
        "bootstrap": "^4.5.3",
        "bootstrap-sweetalert": "^1.0.1",
        "cookie-parser": "^1.4.5",
        "css-loader": "^5.0.1",
        "debug": "^4.3.1",
        "dexie": "^3.0.3",
        "express": "^4.17.1",
        "fabric": "^4.2.0",
        "file-loader": "^6.2.0",
        "got": "^11.8.0",
        "jquery": "^3.5.1",
        "knuth-shuffle": "^1.0.8",
        "mini-css-extract-plugin": "^1.3.1",
        "ml5": "^0.6.0",
        "morgan": "^1.10.0",
        "npm-run-all": "^4.1.5",
        "popper.js": "^1.16.1",
        "pug": "^3.0.0",
        "socket.io": "^3.0.3",
        "socket.io-client": "^3.0.3",
        "striptags": "^3.1.1",
        "typeface-pangolin": "1.1.13",
        "url-loader": "^4.1.1",
        "webpack": "^5.6.0",
        "webpack-cli": "^4.2.0"
    },
    "devDependencies": {
        "husky": "^4.3.0",
        "jest": "^26.6.3",
        "nodemon": "^2.0.6",
        "prettier": "^2.2.0",
        "pretty-quick": "^3.1.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "run-s lint:staged test"
        }
    },
    "type": "module"
}
